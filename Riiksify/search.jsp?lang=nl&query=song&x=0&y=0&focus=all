    <div id="rijkshomeDiv"></div><div xmlns="" class="Coulisse" id="menuCoulisseCollection" style="width: 190px; visibility: visible; display: block;"><div class="Item" id="collectionMenu" onmouseout="NavOut();" onmouseover="NavOver(this);" style="visibility: visible; display: block; left: 0px; width: 190px;"><div class="Inner" style="top: 0px; width: 190px; height: 580px; left: 0px; z-index: 1000;"><div id="navigation"><img alt="" height="18" src="/_img/collectionnav/pixel.gif" width="1"/><script xmlns="http://www.w3.org/1999/xhtml" type="text/javascript"><!--
        
    /* handles submenu display and hide */
    var submenuTimer = null;
    var hideTimer = null;
    var submenuActive = false;
    var submenuParent = null;

    function submenu(el, cancel) {
      if (submenuTimer) {
        clearTimeout(submenuTimer);
        submenuTimer=null;
      }
      if (cancel) return; // done.

      submenuParent = el;
      if (submenuActive) {
        // direct.
        showSubmenu();
      } else {
        submenuTimer = setTimeout("showSubmenu();", 500);
      }
    }

    function showSubmenu() {

      if (submenuParent) {
        unhideSubmenu();
        var submenu = document.getElementById("sub"+submenuParent.id);
        if (submenu != submenuActive) _hideSubmenu();
        submenu.style.display="block";
        submenuActive = submenu;

        // right top-setting
        var spacer=document.getElementById("spacer_sub"+submenuParent.id);
        var top = getTop(submenuParent)-5;
        spacer.style.height=top+"px";
      }

    }

    function hideSubmenu() {
      if (!hideTimer) {
        hideTimer = setTimeout("_hideSubmenu();",50);
      }
    }

    function unhideSubmenu() {
      if (hideTimer) clearTimeout(hideTimer);
      hideTimer=null;
    }

    function _hideSubmenu() {
      if (submenuActive) {
        submenuActive.style.display="none";
        submenuActive = null;
      }
      hideTimer=null;
    }

    function getTop(el) {
      if (el.offsetParent) return getTop(el.offsetParent)*1 + el.offsetTop*1;
      return el.offsetTop;
    }
    
        //
